local attachment = Instance.new("Attachment")
attachment.Position = Vector3.new(0, 0, 2)
attachment.Parent = hrp

local steam = Instance.new("ParticleEmitter")
steam.Texture = "rbxassetid://258128463"  -- soft smoke/wind texture
steam.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(180, 180, 180)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(130, 130, 130))
}
steam.Size = NumberSequence.new{
    NumberSequenceKeypoint.new(0, 5),
    NumberSequenceKeypoint.new(0.5, 8),
    NumberSequenceKeypoint.new(1, 10)
}
steam.Transparency = NumberSequence.new{
    NumberSequenceKeypoint.new(0, 0.85),   -- start very transparent (mostly invisible)
    NumberSequenceKeypoint.new(0.2, 0.6),  -- fade in gently
    NumberSequenceKeypoint.new(0.5, 0.5),  -- almost fully visible but soft
    NumberSequenceKeypoint.new(0.8, 0.75), -- start fading out gently
    NumberSequenceKeypoint.new(1, 1)       -- fully invisible at end
}
steam.Lifetime = NumberRange.new(1, 1.3)
steam.Rate = 0 -- no continuous spawn
steam.Speed = NumberRange.new(18, 24)
steam.EmissionDirection = Enum.NormalId.Back
steam.VelocitySpread = 90
steam.Rotation = NumberRange.new(-25, 25)
steam.RotSpeed = NumberRange.new(-50, 50)
steam.LightEmission = 0.3
steam.ZOffset = 3
steam.Parent = attachment

steam:Emit(40)  -- fewer particles, less “spammy”

delay(1.6, function()
    attachment:Destroy()
end)


delay(1.8, function()

    Anim:Stop()

end)


    end

end

