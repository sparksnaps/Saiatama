local TweenService = game:GetService("TweenService")
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local root = char:WaitForChild("HumanoidRootPart")

-- White aura on arm
local t = {}
for i = 1, 3 do
    for _, limb in ipairs({"Right Arm", "Left Arm"}) do
        local att = Instance.new("Attachment")
        for _, v in ipairs(game.ReplicatedStorage.Emotes.cursedEnergy:GetChildren()) do
            local k = v:Clone()
            k.Parent = att
        end
        table.insert(t, att)
        att.Parent = char[limb]
        att.CFrame = CFrame.new(0, -1.1, 0)
        for _, v in ipairs(att:GetDescendants()) do
            if v:IsA("ParticleEmitter") then
                v:Emit(3)
                v.Enabled = true
                v.Color = ColorSequence.new{
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
                }
            end
        end
    end
end

-- Clean up after 3 seconds
task.delay(3, function()
    for _, a in ipairs(t) do
        for _, v in ipairs(a:GetDescendants()) do
            if v:IsA("ParticleEmitter") then
                v.Enabled = false
            end
        end
    end
    task.wait(2)
    for _, v in ipairs(t) do
        v:Destroy()
    end
end)

-- Camera shake
local Intensity = 1
local ShakeLength = 3
local FadeLength = 5

local rn = Random.new()
local int = Instance.new("NumberValue")
int.Value = Intensity
local b
b = game:GetService("RunService").Heartbeat:Connect(function()
    char.Humanoid.CameraOffset = Vector3.new(
        rn:NextNumber(-int.Value, int.Value),
        rn:NextNumber(-int.Value, int.Value),
        rn:NextNumber(-int.Value, int.Value)
    )
end)

task.wait(ShakeLength)
local p = TweenService:Create(int, TweenInfo.new(FadeLength, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Value = 0})
p:Play()
p.Completed:Wait()
b:Disconnect()


    end

end

